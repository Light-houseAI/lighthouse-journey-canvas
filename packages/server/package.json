{
  "name": "@journey/server",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "build": "node esbuild.config.js",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "postbuild": "cp -r ../ui/dist/client ./dist/public",
    "dev": "tsx --watch --watch-preserve-output src/index.ts",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:init-vector": "tsx scripts/init-vector-db.ts",
    "db:sync-vector": "tsx scripts/sync-vector-db.ts",
    "db:sync:pgvector": "tsx scripts/sync-timeline-to-pgvector.ts",
    "db:test:pgvector": "tsx scripts/test-pgvector-setup.ts",
    "db:populate-orgs": "NODE_ENV=development tsx scripts/populate-organizations.ts",
    "db:populate-members": "NODE_ENV=development tsx scripts/populate-org-members-meta.ts",
    "db:inspect-timeline": "NODE_ENV=development tsx scripts/inspect-timeline-data.ts",
    "test:experience-sync": "tsx scripts/test-experience-sync.ts",
    "test:api": "cd server && vitest tests/api",
    "test:api:watch": "cd server && vitest tests/api --watch",
    "test:api:run": "cd server && vitest tests/api --run",
    "generate:api-docs": "node scripts/generate-api-docs.js",
    "generate:swagger": "node scripts/generate-swagger.js",
    "generate:postman": "node scripts/generate-postman-collection.js"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.27",
    "@journey/schema": "workspace:*",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.5",
    "ai": "^5.0.39",
    "awilix": "^12.0.5",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "commander": "^14.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.39.1",
    "express": "^4.21.2",
    "immer": "^10.1.1",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.1.5",
    "openai": "^5.10.2",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "ws": "^8.18.0",
    "zod": "^3.25.76",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.55.0",
    "@replit/vite-plugin-cartographer": "^0.2.7",
    "@replit/vite-plugin-runtime-error-modal": "^0.0.3",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.1.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/express": "4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/node": "20.16.11",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.30.4",
    "esbuild": "^0.25.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "msw": "^2.11.2",
    "postcss": "^8.4.47",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "supertest": "^7.1.4",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.1",
    "typescript": "5.6.3",
    "vite": "^5.4.19",
    "vite-plugin-static-copy": "^3.1.2",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8"
  },
  "msw": {
    "workerDirectory": [
      "client/public"
    ]
  }
}
