{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --minify --sourcemap --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:generate": "cd server && drizzle-kit generate",
    "db:migrate": "tsx server/scripts/run-migrations.ts",
    "db:migrate:status": "cd server && drizzle-kit check",
    "db:push": "drizzle-kit push",
    "db:init-vector": "tsx server/scripts/init-vector-db.ts",
    "db:sync-vector": "tsx server/scripts/sync-vector-db.ts",
    "db:migrate:pgvector": "psql $DATABASE_URL -f server/migrations/20250909175329_add-pgvector-graphrag-tables.sql",
    "db:sync:pgvector": "tsx server/scripts/sync-timeline-to-pgvector.ts",
    "db:test:pgvector": "tsx server/scripts/test-pgvector-setup.ts",
    "db:populate-orgs": "NODE_ENV=development tsx server/scripts/populate-organizations.ts",
    "db:populate-members": "NODE_ENV=development tsx server/scripts/populate-org-members-meta.ts",
    "db:inspect-timeline": "NODE_ENV=development tsx server/scripts/inspect-timeline-data.ts",
    "test:experience-sync": "tsx server/scripts/test-experience-sync.ts",
    "test:api": "node run-api-tests.js",
    "test:api:watch": "nodemon --exec \"node run-api-tests.js\" --watch server --ext js,ts",
    "test:run_server": "USE_TEST_DB=true npm run dev",
    "test:setup": "./setup-local-testing.sh",
    "test:api:with-server": "NODE_ENV=test tsx server/test-server-simple.ts & sleep 3 && node run-api-tests.js; pkill -f tsx",
    "test": "NODE_ENV=test vitest",
    "test:server": "cd server && vitest",
    "test:client": "cd client && vitest",
    "test:e2e": "npm run test:e2e:setup && cd client && playwright test --project=chromium-auth",
    "test:e2e:all": "cd client && playwright test",
    "test:e2e:ui": "cd client && playwright test --ui",
    "test:e2e:debug": "cd client && playwright test --debug --headed --project=chromium-auth",
    "test:e2e:report": "cd client && playwright show-report",
    "test:e2e:setup": "npx tsx scripts/setup-test-data.ts",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "pre-commit": "lint-staged"
  },
